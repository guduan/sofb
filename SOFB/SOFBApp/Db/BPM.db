record(aSub, "$(SR):${CELL}-SOFB{BPM}bpmX_aSub")
{
	field(SNAM, "bpmAverage")
	field(INPA, "$(SR):${CELL}-BI{BPM:1}Pos:XwUsrOff-Calc")
	field(FTA,  "FLOAT")	
	field(NOA,  "1")
	field(INPB, "$(SR):${CELL}-BI{BPM:2}Pos:XwUsrOff-Calc")
	field(FTB,  "FLOAT")	
	field(NOB,  "1")
	field(INPC, "$(SR):${CELL}-BI{BPM:3}Pos:XwUsrOff-Calc")
	field(FTC,  "FLOAT")	
	field(NOC,  "1")
	field(INPD, "$(SR):${CELL}-BI{BPM:4}Pos:XwUsrOff-Calc")
	field(FTD,  "FLOAT")	
	field(NOD,  "1")
	field(INPE, "$(SR):${CELL}-BI{BPM:5}Pos:XwUsrOff-Calc")
	field(FTE,  "FLOAT")	
	field(NOE,  "1")
	field(INPF, "$(SR):${CELL}-BI{BPM:6}Pos:XwUsrOff-Calc")
	field(FTF,  "FLOAT")	
	field(NOF,  "1")

	field(INPG, "$(SR)-SOFB{BPM}NSOFB")
	field(FTG,  "FLOAT")	
	field(NOG,  "1")
	field(INPH, "$(SR)-SOFB{BPM}NORM")
	field(FTH,  "FLOAT")	
	field(NOH,  "1")
	field(INPI, "$(SR)-SOFB{BPM}RESET")
	field(FTI,  "CHAR")	
	field(NOI,  "1")
	field(INPJ, "$(SR)-SOFB{BPM}cycles")
	field(FTJ,  "FLOAT")	
	field(NOJ,  "1")

	field(INPK, "$(SR):${CELL}-BI{BPM:1}Enbl:XOC-Cmd")
	field(FTK,  "CHAR")	
	field(INPL, "$(SR):${CELL}-BI{BPM:2}Enbl:XOC-Cmd")
	field(FTL,  "CHAR")	
	field(INPM, "$(SR):${CELL}-BI{BPM:3}Enbl:XOC-Cmd")
	field(FTM,  "CHAR")	
	field(INPN, "$(SR):${CELL}-BI{BPM:4}Enbl:XOC-Cmd")
	field(FTN,  "CHAR")	
	field(INPO, "$(SR):${CELL}-BI{BPM:5}Enbl:XOC-Cmd")
	field(FTO,  "CHAR")	
	field(INPP, "$(SR):${CELL}-BI{BPM:6}Enbl:XOC-Cmd")
	field(FTP,  "CHAR")	


	field(OUTA, "$(SR):${CELL}-SOFB{BPM}xSA PP")
	field(NOVA, "6")
	field(FTVA, "FLOAT")

	field(OUTB, "$(SR):${CELL}-SOFB{BPM}xSumSOFB PP")
	field(NOVB, "6")
	field(FTVB, "FLOAT")

	field(OUTC, "$(SR):${CELL}-SOFB{BPM}xSumORM PP")
	field(NOVC, "6")
	field(FTVC, "FLOAT")

	field(OUTD, "$(SR):${CELL}-SOFB{BPM}xSOFB PP")
	field(NOVD, "6")
	field(FTVD, "FLOAT")

	field(OUTE, "$(SR):${CELL}-SOFB{BPM}xORM PP")
	field(NOVE, "6")
	field(FTVE, "FLOAT")
	field(FLNK, "$(SR):${CELL}-SOFB{BPM}bpmY_aSub")
}


