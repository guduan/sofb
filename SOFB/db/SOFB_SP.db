record(aSub, "$(SR)-SOFB2{}sofb:pid_aSub") 
{
#	field(SCAN, "1 second")
	field(SNAM, "pidSP")
	field(INPA, "$(SR)-SOFB2{BPM}sofb:RV")
	field(FTA, "FLOAT")
	field(NOA, "129960")

	field(INPB, "$(SR)-SOFB2{BPM}orb:xSOFB")
	field(FTB, "FLOAT")
	field(NOB, "180")
	field(INPC, "$(SR)-SOFB2{BPM}orb:ySOFB CP")  #must be CP
	field(FTC, "FLOAT")
	field(NOC, "180")

	field(INPD, "$(SR)-SOFB2{BPM}orb:xRef")
	field(FTD, "FLOAT")
	field(NOD, "180")
	field(INPE, "$(SR)-SOFB2{BPM}orb:yRef")
	field(FTE, "FLOAT")
	field(NOE, "180")

	field(INPF, "$(SR)-SOFB2{}pidRst")
	field(FTF, "CHAR")
	field(NOF, "1")

	field(INPG, "$(SR)-SOFB2{}Kp")
	field(FTG, "FLOAT")
	field(NOG, "1")
	field(INPH, "$(SR)-SOFB2{}Ki")
	field(FTH, "FLOAT")
	field(NOH, "1")
	field(INPI, "$(SR)-SOFB2{}Kd")
	field(FTI, "FLOAT")
	field(NOI, "1")

	field(INPJ, "$(SR)-SOFB2{}Enable")
	field(FTJ, "CHAR")
	field(NOJ, "1")

	field(INPK, "$(SR)-SOFB2{}ManuSet")
	field(FTK, "CHAR")
	field(NOK, "1")

	field(INPL, "$(SR)-SOFB2{}deltaSP")
	field(FTL, "FLOAT")
	field(NOL, "360")

	field(INPM, "$(SR)-SOFB2{}SP")
	field(FTM, "FLOAT")
	field(NOM, "360")

	field(INPN, "$(SR)-SOFB2{}sofb:pid_I_Decay")
	field(FTN, "FLOAT")
	field(NON, "1")

	field(OUTA, "$(SR)-SOFB2{}deltaSP PP")
	field(NOVA, "360")
	field(FTVA, "FLOAT")

	field(OUTB, "$(SR)-SOFB2{BPM}sofb:deltaRow PP")
	field(NOVB, "360")
	field(FTVB, "FLOAT")

	field(OUTC, "$(SR)-SOFB2{BPM}X:RMSDelta-I PP")
	field(NOVC, "1")
	field(FTVC, "FLOAT")	

	field(OUTD, "$(SR)-SOFB2{BPM}Y:RMSDelta-I PP")
	field(NOVD, "1")
	field(FTVD, "FLOAT")

	field(OUTE, "$(SR)-SOFB2{BPM}sofb:deltaXY-RMS PP")
	field(NOVE, "1")
	field(FTVE, "FLOAT")

	field(OUTF, "$(SR)-SOFB2{}SP PP")
	field(NOVF, "360")
	field(FTVF, "FLOAT")

	field(OUTG, "$(SR)-SOFB2{}deltaSum PP")
	field(NOVG, "360")
	field(FTVG, "FLOAT")

	field(OUTH, "$(SR)-SOFB2{}deltaDiff PP")
	field(NOVH, "360")
	field(FTVH, "FLOAT")

#	field(FLNK,"$(SR):C01-SOFB2{}corSP_aSub")
}

record(ao, "$(SR)-SOFB2{BPM}X:RMSDelta-I") {
	field(DESC, "RMS of X orbit delta")
}

record(ao, "$(SR)-SOFB2{BPM}Y:RMSDelta-I") {
	field(DESC, "RMS of Y orbit delta")
}

record(ao, "$(SR)-SOFB2{BPM}sofb:deltaXY-RMS") {
	field(DESC, "RMS of X Y orbit delta")
}

record(ao, "$(SR)-SOFB2{}sofb:pid_I_Decay") {
	field(VAL, "0.5")
}

