record(waveform, "$(p)SR:APHLA:SOFB{BPM}Pos:XwUsrOff-Calc") {
   field(FTVL, "DOUBLE")
   field(NELM, "211")
}

record(waveform, "$(p)test:range:inp_") {
   field(FTVL, "SHORT")
   field(NELM, "211")
}
record(waveform, "$(p)test:range:out_") {
   field(FTVL, "SHORT")
   field(NELM, "211")
}

record(aSub, "$(p)SR:APHLA:SOFB{BPM}_shuffle_asub_") {
   field(INPA, "$(p)SR:APHLA:SOFB{BPM}Index")
   field(NOA, "211")
   field(FTA, "SHORT")

   field(INPB, "$(p)SR:APHLA:SOFB{BPM}Pos:XwUsrOff-Calc_")
   field(FTB, "DOUBLE")
   field(NOB, "211")
   field(OUTB, "$(p)SR:APHLA:SOFB{BPM}Pos:XwUsrOff-Calc PP")
   field(FTVB, "DOUBLE")
   field(NOVB, "211")

   field(INPC, "$(p)test:range:inp_")
   field(FTC, "DOUBLE")
   field(NOC, "211")
   field(OUTC, "$(p)test:range:out_")
   field(FTVC, "DOUBLE")
   field(NOVC, "211")

   field(SNAM, "shuffleWaveforms")
}
